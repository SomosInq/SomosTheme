{% comment %}
UGC / Influencer Proof - Fashion Store
{% endcomment %}
{% render 'theme-animations-config' %}
<section class="ugc-influencer section spacing-style color-{{ section.settings.color_scheme }}">
  <div class="ugc-influencer__container">
    <h3>As seen on</h3>
    <div class="ugc-influencer__logos">
      <img src="https://placehold.co/100x40?text=Vogue" alt="Vogue" />
      <img src="https://placehold.co/100x40?text=Elle" alt="Elle" />
      <img src="https://placehold.co/100x40?text=GQ" alt="GQ" />
      <img src="https://placehold.co/100x40?text=Insta" alt="Instagram Influencer" />
    </div>
  </div>
</section>
{% schema %}{
  "name": "UGC / Influencer Proof",
  "settings": [
    { "type": "color_scheme", "id": "color_scheme", "label": "Color scheme", "default": "scheme-1" }
  ],
  "presets": [ { "name": "UGC / Influencer Proof" } ]
}{% endschema %}